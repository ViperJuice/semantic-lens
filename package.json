{
  "name": "semantic-lens",
  "version": "0.1.0",
  "type": "module",
  "description": "A semantic graph analysis tool for codebase understanding",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "semantic-lens": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc && cp src/schema/semantic-graph-bundle.schema.json dist/schema/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src tests",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate": "node dist/cli/index.js validate"
  },
  "keywords": [
    "semantic-analysis",
    "code-graph",
    "static-analysis"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/cytoscape": "^3.21.9",
    "ajv": "^8.12.0",
    "ajv-draft-04": "^1.0.0",
    "ajv-formats": "^2.1.1",
    "cors": "^2.8.5",
    "cytoscape": "^3.33.1",
    "elkjs": "^0.9.3",
    "express": "^4.18.2",
    "graphology": "^0.26.0",
    "graphology-communities-louvain": "^2.0.2",
    "graphology-layout-forceatlas2": "^0.10.1",
    "neo4j-driver": "^5.17.0",
    "sigma": "^3.0.2",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.55.0",
    "graphology-types": "^0.24.8",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
